<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_saving"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/bg2color"
    tools:context="com.example.top.test.saving">

    <TextView
        android:text="Krungsri bank"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Krungsribanktext"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="18dp"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />


    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/kasikornlogo"
        android:id="@+id/imageView7"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="$$$$$$$$$$ "
        android:textSize="20dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_below="@+id/Krungsribanktext"
        android:layout_alignLeft="@+id/Krungsribanktext"
        android:layout_alignStart="@+id/Krungsribanktext" />

    <TextView
        android:text="$$$$$$$$$$ "
        android:textSize="20dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_below="@+id/Kasikornbank"
        android:layout_toLeftOf="@+id/imageView7"
        android:layout_toStartOf="@+id/imageView7" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ssssss"
        android:id="@+id/imageView9"
        android:layout_below="@+id/imageView6"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />

    <TextView
        android:textSize="20dp"
        android:text="$$$$$$$$$$ "
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_marginBottom="21dp"
        android:layout_above="@+id/imageView9"
        android:layout_alignLeft="@+id/Krungthaibank"
        android:layout_alignStart="@+id/Krungthaibank" />

    <TextView
        android:text="$$$$$$$$$$ "
        android:textSize="20dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/SCB"
        android:layout_alignLeft="@+id/SCB"
        android:layout_alignStart="@+id/SCB" />

    <TextView
        android:text="Kasikorn bank"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Kasikornbank"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <TextView
        android:text="Saving account compare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="25sp" />

    <TextView
        android:text="Banks with the best interest: "
        android:textSize="14dp"
        android:textColor="@android:color/holo_red_dark"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/ktb_01"
        android:id="@+id/imageView6"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/kslogo"
        android:layout_marginTop="19dp"
        android:id="@+id/imageView2"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Krungthai bank"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Krungthaibank"
        android:layout_above="@+id/textView3"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <TextView
        android:text="SCB Bank"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/SCB"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/imageView6"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />


</RelativeLayout>
